{
  "manifest_version": 2,
  "name": "elm-chrome-extension-starter",
  "version": "0.0.2",
  "permissions": [],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "elm-chrome-extension-starter",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ]
}
